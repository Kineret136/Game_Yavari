    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>if vspeed &gt; 0{
    Fall = true;
}else{
    Fall = false;
}
if vspeed &lt; 0{
    Up = true;
}else{
    Up = false;
}
if(fuera){
    //x = 64;
    //y = 64;
    x = xstart;
    y = ystart;
    fuera = false;
}


/// Gravedad y Movimiento
//Gravedad
/*if place_free(x, y+1){
    gravity = 1;
}else{
    gravity = 0;
}*/
//if vspeed &gt;= 15 {vspeed = 15;} //Limite de velocidad vertical

//Dialogo inicial
switch (mostrar_dialogo){
    case 0: 
        //MOVIMIENTOS
        
        //variables de velocidad
        V = 10; 
        Salto = 10;
        
        //Lectura de las key
        var Der = keyboard_check(vk_right);
        var Izq = keyboard_check(vk_left);
        
        //Evitar que se presionen ambas teclas
        if Izq {Der = false};
        if Der {Izq = false};
        
        //Disparo
        var Shoot = keyboard_check_pressed(vk_space);
        
        // Acciones para la tecla Derecha
        if Der &amp;&amp; place_free(x+V, y)
            {
                x += V; //Moverlo
                chequear_pressed = true;
                
                //Disparo
                V_disparo = 5;
                numScaleX = 1;
                direct_prs = true; //dirección del disparo
                
                scrChangeSprite(Fall, Up, 1, 1);
                
                
            }
        
        //Acciones para la izquierda
        if Izq  &amp;&amp; place_free(x-V, y)
            {
                x -= V; //Movimiento
                chequear_pressed = true;
                //Disparos
                numScaleX = -1;
                V_disparo = 5;
                direct_prs = false;//dirección del disparo
                
                scrChangeSprite(Fall, Up, 1, -1);

                
            }
            //Stop
            if !keyboard_key
            {
                //Sprite que se muestra
                scrChangeSprite(Fall, Up, 1, 0);

            }
            
            //Acciones de Salto
            var S = keyboard_check_pressed(vk_up);
            if S{
                //V_disparo = 5;
                sprite_index = spr_player_jump;
                vspeed = -Salto;
            }
            
            //numMove = Der + Izq;
            //numVX = numMove*V_disparo;
            
            //ACciones de disparo
            if(Shoot &amp;&amp; direct_prs){
                    numVX = 1*V_disparo;
                    insShoot = instance_create(x,y-13, obj_shoot);
                    insShoot.numVX *= 1; 
                    audio_play_sound(snd_shoot,2,false);
            }else if (Shoot &amp;&amp; !direct_prs){
                numVX = -1*V_disparo;
                insShoot = instance_create(x,y-13, obj_shoot_izq);
                insShoot.numVX *= -1; 
                audio_play_sound(snd_shoot,2,false);
            }
           
    break;
    case 1: 
        //Start game y presentación del dialogo
        V = 0;
        Salto = 0; 
        walk = 0;
        sprite_index = spr_player_parado;
        image_xscale = 1;
        break;
}
//Puntos minimos para pasar de nivel 
nivel1=20;
nivel2=30;
nivel3=40;
puntosminimos=20;

//Pase de nivel
if(global.n_nivel==1 &amp;&amp; global.score_room &gt;= 20){
    puntosminimos=20;
    next = true;
}else if(global.n_nivel==2 &amp;&amp; global.score_room &gt;= 30){
    puntosminimos=30;
    next = true;
}else if(global.n_nivel==3 &amp;&amp; global.score_room &gt;= 40){
    puntosminimos=40;
    next = true;
}else{
    next = false;
}

//Score global &lt;0
if(global.score_total &lt;0){
    obj_vidas.vidamax = 0;
}

//Perdiste
if (
    instance_number(obj_chinchilla) &lt; 1 &amp;&amp; 
    instance_number(obj_moneda) &lt; 1 &amp;&amp; 
    instance_number(obj_alpaca) &lt; 1 &amp;&amp; instance_number(obj_obra) &lt; 1 &amp;&amp; 
    global.score_room &lt;= puntosminimos
    ) // si hay cero instancias del objeto indicado...
{
    if(global.score_total &gt; global.hiscore){
        global.hiscore = global.score_total;
        ini_open("score.ini");
        ini_write_real("score", "hiscore", global.hiscore);
        ini_close();
    }
    //room de score total 
    show_question("Perdiste! Necesitabas "+puntosminimos+" puntos como mínimo para pasar este nivel");
    mostrar_dialogo = 1; //Sin movimientos al jugador 
    
}

</string>
          </argument>
        </arguments>
