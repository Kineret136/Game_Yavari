            <kind>1</kind>
            <string>///Gravedad y saltos &amp;&amp; Movimiento de plataforma
///Movimiento de plataforma
var plataform = collision_rectangle(x-15,y,x+15, y+1,obj_mov,true,true);
if(plataform &amp;&amp; vspeed == 0){
    x+=plataform.hspeed;
    y+=plataform.vspeed;
    //y+=plataform.y;
}
///Gravedad y saltos
if (!collision_rectangle(x-15,y,x+15,y+1,obj_plataforma,false,false)){
    gravity = 1;
    
};
if (vspeed &gt; 0 ){
    var ground = collision_rectangle(x-15,y,x+15, y+vspeed,obj_plataforma,false,false);
    if(ground){
        y=ground.y;
        vspeed = 0;
        gravity = 0;
    }
}else if (vspeed &lt; 0 ){
    var ceiling = collision_rectangle(x-15,y-49,x+15, y-49+vspeed,obj_plataforma,false,false);
    if(ceiling &amp;&amp; ceiling.oneSided == false){
        y=ceiling.y + ceiling.sprite_height + 49;
        vspeed = 0;
    }
}

